t\t\t\t\t\t</li>\n\t\t\t\t\t</ul>\n\t\t\t\t</div>\n\t\t\t\t<div>\n\t\t\t\t\t<li>Utilitaires:</li>\n\t\t\t\t\t<ul>\n\t\t\t\t\t\t<div v-for=\"(util, index3) in utils\" :key=\"'util-' + index3\">\n\t\t\t\t\t\t\t<li>\n\t\t\t\t\t\t\t\t<a :href=\"util.link\">{{ util.title }}</a>\n\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</ul>\n\t\t\t\t</div>\n\t\t\t</ul>\n\t\t</div>\n\t</section>\n</template>\n\n<script setup lang=\"ts\">\nimport { defineComponent, onMounted, PropType, ref } from \"vue\";\nimport axios, { AxiosResponse } from \"axios\";\n\ninterface Student {\n\t_id: string;\n\tname: string;\n\tclass: string;\n\tcours: {\n\t\t_id: string;\n\t\t__v: number;\n\t\tmonth: string;\n\t\tlessons: {\n\t\t\t_id: string;\n\t\t\tday: string;\n\t\t\ttitle: string;\n\t\t\tlink: string;\n\t\t}[];\n\t}[];\n}\n\ninterface Util {\n\t_id: string;\n\ttitle: string;\n\tlink: string;\n\t__v: number;\n}\n\n\nexport default defineComponent({\n\tname: \"Students2\",\n\n\tprops: {\n\t\tname: {\n\t\t\ttype: String as PropType<string>,\n\t\t\trequired: true,\n\t\t},\n\t},\n\n\tsetup(props) {\n\t\tlet student = ref<null | Student>(null);\n\t\tlet utils = ref<null | Util[]>(null);\n\n\t\tconst capitalize = (str: string): string => {\n\t\t\tif (str === \"\") return \"\";\n\t\t\treturn str.charAt(0).toUpperCase() + str.slice(1);\n\t\t};\n\n\t\tonMounted(() => {\n\t\t\taxios\n\t\t\t\t.get(`http://localhost:3000/api/student/${props.name}`)\n\t\t\t\t.then((response: AxiosResponse<{ student: Student }>) => {\n\t\t\t\t\tstudent.value = response.data.student;\n\t\t\t\t});\n\t\t\taxios\n\t\t\t\t.get(`https://sheltered-basin-99154.herokuapp.com/api/studentUtils`)\n\t\t\t\t.then((response: AxiosResponse<Util[]>) => {\n\t\t\t\t\tutils.value = response.data;\n\t\t\t\t});\n\t\t});\n\n\t\treturn {\n\t\t\tstudent,\n\t\t\tutils,\n\t\t\tcapitalize,\n\t\t};\n\t},\n});\n</script>\n\n<style lang=\"scss\" scoped>\nsection {\n\tdiv {\n\t\tmargin-bottom: 20px;\n\t}\n}\n</style>\n"]}]}