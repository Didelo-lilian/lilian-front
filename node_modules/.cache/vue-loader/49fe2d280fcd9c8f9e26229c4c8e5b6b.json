          <li v-for=\"(lesson, index2) in cours.lessons\" :key=\"'lesson-'+ index2\">\n            <a :href=\"lesson.link\">{{ lesson.day }} - {{ lesson.title }}</a>\n          </li>\n        </ul>\n      </div>\n      <div>\n        <li>Utilitaires:</li>\n        <ul>\n          <div v-for=\"(util, index3) in utils\" :key=\"'util-'+ index3\">\n            <li>\n              <a :href=\"util.link\">{{ util.title }}</a>\n            </li>\n          </div>\n        </ul>\n      </div>\n    </ul>\n</div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent } from \"vue\";\n\ninterface LessonInterface {\n\tday: string;\n\ttitle: string;\n\tlink: string;\n}\n\ninterface CoursInterface {\n\tmonth: string;\n\tlessons: LessonInterface[];\n}\n\ninterface StudentInfoInterface {\n\tname: string;\n\tclass: string;\n\tcours: CoursInterface[];\n}\n\ninterface UtilInterface {\n\ttitle: string;\n\tlink: string;\n}\n\ninterface StudentsInfoInterface {\n\tstudents: StudentInfoInterface[];\n\tutils: UtilInterface[];\n}\n\nconst amandineInfo: StudentInfoInterface = {\n\tname: \"amandine\",\n\tclass: \"2nde\",\n\tcours: [\n\t\t{\n\t\t\tmonth: \"Mathématiques\",\n\t\t\tlessons: [\n\t\t\t\t{\n\t\t\t\t\tday: \"21/10\",\n\t\t\t\t\ttitle: \"Entrainement Fraction\",\n\t\t\t\t\tlink: \"./cours/amandine/exo-fraction.pdf\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"25/11\",\n\t\t\t\t\ttitle: \"Entrainement Vecteur\",\n\t\t\t\t\tlink: \"./cours/amandine/exo-vecteur.pdf\",\n\t\t\t\t},\n\t\t\t],\n\t\t},\n\t],\n};\n\nconst kevinJInfo: StudentInfoInterface = {\n\tname: \"kevin-j\",\n\tclass: \"6ème\",\n\tcours: [\n\t\t{\n\t\t\tmonth: \"Janvier\",\n\t\t\tlessons: [\n\t\t\t\t{\n\t\t\t\t\tday: \"24\",\n\t\t\t\t\ttitle: \"Cours Nombre décimaux\",\n\t\t\t\t\tlink: \"cours/kevin-j/24-01-kevin-j.pdf\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"24\",\n\t\t\t\t\ttitle: \"Exercice calcul nombre décimaux\",\n\t\t\t\t\tlink: \"https://forms.gle/6iPu3JCByMBztR9t8\",\n\t\t\t\t},\n\t\t\t],\n\t\t},\n\t\t{\n\t\t\tmonth: \"Fevrier\",\n\t\t\tlessons: [\n\t\t\t\t{\n\t\t\t\t\tday: \"07\",\n\t\t\t\t\ttitle: \"Cours Angle et Correction DS\",\n\t\t\t\t\tlink: \"cours/kevin-j/07-02-kevin-j.pdf\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"07\",\n\t\t\t\t\ttitle: \"Exercice opérations\",\n\t\t\t\t\tlink: \"cours/kevin-j/exo-operations.pdf\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"07\",\n\t\t\t\t\ttitle: \"Exercice mesure angle\",\n\t\t\t\t\tlink: \"cours/kevin-j/exo-mesure-angle.pdf\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"14\",\n\t\t\t\t\ttitle: \"Cours Opération\",\n\t\t\t\t\tlink: \"cours/kevin-j/14-02-kevin-j.pdf\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"28\",\n\t\t\t\t\ttitle: \"Astuce pour gagner au jeu de la course\",\n\t\t\t\t\tlink: \"cours/kevin-j/28-02-kevin-j.pdf\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"28\",\n\t\t\t\t\ttitle: \"Exercice opération\",\n\t\t\t\t\tlink: \"cours/kevin-j/exo-operations-2.pdf\",\n\t\t\t\t},\n\t\t\t],\n\t\t},\n\t\t{\n\t\t\tmonth: \"Mars\",\n\t\t\tlessons: [\n\t\t\t\t{\n\t\t\t\t\tday: \"07\",\n\t\t\t\t\ttitle: \"Correction EX9 et Cours division Euclidienne\",\n\t\t\t\t\tlink: \"cours/kevin-j/07-03-kevin-j.pdf\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"07\",\n\t\t\t\t\ttitle: \"Exercice division euclidienne\",\n\t\t\t\t\tlink: \"https://forms.gle/ptHbfwB89ijoKFY29\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"14\",\n\t\t\t\t\ttitle: \"Exercice division\",\n\t\t\t\t\tlink: \"https://forms.gle/G62XeYoobRzySHzX8\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"28\",\n\t\t\t\t\ttitle: \"Cours & Exo Polygone\",\n\t\t\t\t\tlink: \"cours/kevin-j/28-03-kevin-j.pdf\",\n\t\t\t\t},\n\t\t\t],\n\t\t},\n\t\t{\n\t\t\tmonth: \"Avril\",\n\t\t\tlessons: [\n\t\t\t\t{\n\t\t\t\t\tday: \"04\",\n\t\t\t\t\ttitle: \"Exo Division / Polygone\",\n\t\t\t\t\tlink: \"https://forms.gle/YMjCnpWGPEJjfBwA9\",\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tday: \"11\",\n\t\t\t\t\ttitle: \"Cours & Exo Priorité d'opération - Unité de temps\",\n\t\t\t\t\tlink: \"cours/kevin-j/11-04-kevin-j.pdf\",\n\t\t\t\t},\n\t\t\t],\n\t\t},\n\t],\n};\n\nconst utilsInfo: UtilInterface[] = [\n\t{\n\t\ttitle: \"Tables (multiplication, addition, soustraction)\",\n\t\tlink: \"cours/utils/php/apprendre_table.php?op=2\",\n\t},\n\t{\n\t\ttitle: \"Entrainement opération\",\n\t\tlink: \"cours/utils/php/entrainement_op.php?op=plus\",\n\t},\n\t{\n\t\ttitle: \"Rendu Exercice\",\n\t\tlink: \"cours/utils/php/rendu_exercice.php\",\n\t},\n];\n\nconst StudentsInfo: StudentsInfoInterface = {\n\tstudents: [\n\t\tamandineInfo,\n\t\tkevinJInfo,\n\t],\n\tutils: utilsInfo,\n};\n\nexport default defineComponent({\n\tname: \"Students\",\n\tprops: {\n\t\tid: {\n\t\t\ttype: String,\n\t\t\trequired: true,\n\t\t},\n\t},\n\tcomputed: {\n\t\tstudents() {\n      let students: StudentInfoInterface = StudentsInfo.students[0];\n\t\t\tObject.values(StudentsInfo.students).forEach(stud => {\n        console.log(stud.name, this.id, stud.name == this.id, stud.name === this.id);\n\t\t\t\tif (stud.name === this.id) {\n\t\t\t\t\tstudents =  stud;\n\t\t\t\t}\n\t\t\t});\n\n\t\t\treturn students;\n\t\t},\n\t\tutils() {\n\t\t\treturn StudentsInfo.utils;\n\t\t},\n\t},\n\tmethods: {\n\t\tupperFirstLetter(str: string) {\n\t\t\treturn str.charAt(0).toUpperCase() + str.slice(1);\n\t\t},\n\t},\n});\n</script>\n\n<style scoped lang=\"scss\"></style>\n"]}]}